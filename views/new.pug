extends ./layout.pug

block title
  title Homepage

block body
  div#container
    div#header
      h1 _ARCHITEKT
      p#welcomeNote Welcome to our online portfolio, we are a small group of passionate designers and architects, looking to change and create amazing digital images to inspire others to follow in our innovative steps. Free Architect WordPress Theme
    div#form
      div#newPhoto
        h3 New Photo
      div#formSection
        form(method='post' action='#')
          p.field Title
          input(name='title')
          p.field Author
          input(name ='author')
          p.field Link
          input(name ='link')
          p.field Description
          input(name ='description')
          br
          br
          button#send Enter
          br
          br